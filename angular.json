{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              // "src/styles.css",
              "src/assets/css/bootstrap/bootstrap-grid.css",
              "src/assets/css/bootstrap/bootstrap-reboot.css",
              "src/assets/css/css/bootstrap-reboot.css",
              "src/assets/css/css/mixins/_text-hide.css",
              "src/assets/css/animate.css",
              "src/assets/css/aos.css",
              "src/assets/css/bootstrap-datepicker.css",
              // "src/assets/css/icomoon.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/icomoon.css",
              "src/assets/css/ionicons.min.css",
              "src/assets/css/jquery.timepicker.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/open-iconic-bootstrap.min.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/owl.theme.default.min.css",
              "src/assets/css/style.css",
              "src/assets/scss/style.scss",
              "src/assets/scss/bootstrap/bootstrap.scss",
              "src/assets/scss/bootstrap/_variables.scss",
              "src/assets/scss/bootstrap/_functions.scss",
              "src/assets/scss/bootstrap/_mixins.scss",
              "src/assets/scss/bootstrap/_root.scss",
              "src/assets/scss/bootstrap/_reboot.scss",
              "src/assets/scss/bootstrap/_type.scss",
              "src/assets/scss/bootstrap/_images.scss",
              "src/assets/scss/bootstrap/_code.scss",
              "src/assets/scss/bootstrap/_grid.scss",
              "src/assets/scss/bootstrap/_tables.scss",
              "src/assets/scss/bootstrap/_forms.scss",
              "src/assets/scss/bootstrap/_buttons.scss",
              "src/assets/scss/bootstrap/_transitions.scss",
              "src/assets/scss/bootstrap/_dropdown.scss",
              "src/assets/scss/bootstrap/_button-group.scss",
              "src/assets/scss/bootstrap/_input-group.scss",
              "src/assets/scss/bootstrap/_custom-forms.scss",
              "src/assets/scss/bootstrap/_nav.scss",
              "src/assets/scss/bootstrap/_navbar.scss",
              "src/assets/scss/bootstrap/_card.scss",
              "src/assets/scss/bootstrap/_breadcrumb.scss",
              "src/assets/scss/bootstrap/_pagination.scss",
              "src/assets/scss/bootstrap/_badge.scss",
              "src/assets/scss/bootstrap/_jumbotron.scss",
              "src/assets/scss/bootstrap/_alert.scss",
              "src/assets/scss/bootstrap/_progress.scss",
              "src/assets/scss/bootstrap/_media.scss",
              "src/assets/scss/bootstrap/_list-group.scss",
              "src/assets/scss/bootstrap/_close.scss",
              "src/assets/scss/bootstrap/_toasts.scss",
              "src/assets/scss/bootstrap/_modal.scss",
              "src/assets/scss/bootstrap/_tooltip.scss",
              "src/assets/scss/bootstrap/_popover.scss",
              "src/assets/scss/bootstrap/_carousel.scss",
              "src/assets/scss/bootstrap/_spinners.scss",
              "src/assets/scss/bootstrap/_utilities.scss",
              "src/assets/scss/bootstrap/_print.scss",
              "src/assets/scss/bootstrap/vendor/_rfs.scss",
              "src/assets/scss/bootstrap/mixins/_deprecate.scss",
              "src/assets/scss/bootstrap/mixins/_breakpoints.scss",
              "src/assets/scss/bootstrap/mixins/_hover.scss",
              "src/assets/scss/bootstrap/mixins/_image.scss",
              "src/assets/scss/bootstrap/mixins/_badge.scss",
              "src/assets/scss/bootstrap/mixins/_resize.scss",
              "src/assets/scss/bootstrap/mixins/_screen-reader.scss",
              "src/assets/scss/bootstrap/mixins/_size.scss",
              "src/assets/scss/bootstrap/mixins/_reset-text.scss",
              "src/assets/scss/bootstrap/mixins/_text-emphasis.scss",
              "src/assets/scss/bootstrap/mixins/_text-hide.scss",
              "src/assets/scss/bootstrap/mixins/_text-truncate.scss",
              "src/assets/scss/bootstrap/mixins/_visibility.scss",
              "src/assets/scss/bootstrap/mixins/_alert.scss",
              "src/assets/scss/bootstrap/mixins/_buttons.scss",
              "src/assets/scss/bootstrap/mixins/_caret.scss",
              "src/assets/scss/bootstrap/mixins/_pagination.scss",
              "src/assets/scss/bootstrap/mixins/_lists.scss",
              "src/assets/scss/bootstrap/mixins/_list-group.scss",
              "src/assets/scss/bootstrap/mixins/_nav-divider.scss",
              "src/assets/scss/bootstrap/mixins/_forms.scss",
              "src/assets/scss/bootstrap/mixins/_table-row.scss",
              "src/assets/scss/bootstrap/mixins/_background-variant.scss",
              "src/assets/scss/bootstrap/mixins/_border-radius.scss",
              "src/assets/scss/bootstrap/mixins/_box-shadow.scss",
              "src/assets/scss/bootstrap/mixins/_gradients.scss",
              "src/assets/scss/bootstrap/mixins/_transition.scss",
              "src/assets/scss/bootstrap/mixins/_clearfix.scss",
              "src/assets/scss/bootstrap/mixins/_grid-framework.scss",
              "src/assets/scss/bootstrap/mixins/_grid.scss",
              "src/assets/scss/bootstrap/mixins/_float.scss",
              "src/assets/scss/bootstrap/utilities/_align.scss",
              "src/assets/scss/bootstrap/utilities/_background.scss",
              "src/assets/scss/bootstrap/utilities/_borders.scss",
              "src/assets/scss/bootstrap/utilities/_clearfix.scss",
              "src/assets/scss/bootstrap/utilities/_display.scss",
              "src/assets/scss/bootstrap/utilities/_embed.scss",
              "src/assets/scss/bootstrap/utilities/_flex.scss",
              "src/assets/scss/bootstrap/utilities/_float.scss",
              "src/assets/scss/bootstrap/utilities/_overflow.scss",
              "src/assets/scss/bootstrap/utilities/_position.scss",
              "src/assets/scss/bootstrap/utilities/_screenreaders.scss",
              "src/assets/scss/bootstrap/utilities/_shadows.scss",
              "src/assets/scss/bootstrap/utilities/_sizing.scss",
              "src/assets/scss/bootstrap/utilities/_stretched-link.scss",
              "src/assets/scss/bootstrap/utilities/_spacing.scss",
              "src/assets/scss/bootstrap/utilities/_text.scss",
              "src/assets/scss/bootstrap/utilities/_visibility.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo"
}